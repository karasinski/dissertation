	year = {2013},
}

@article{phillips_human-animal_2016,
	title = {Human-animal {Teams} {As} an {Analog} for {Future} {Human}-robot {Teams}: {Influencing} {Design} and {Fostering} {Trust}},
	volume = {5},
	issn = {2163-0364},
	shorttitle = {Human-animal {Teams} {As} an {Analog} for {Future} {Human}-robot {Teams}},
